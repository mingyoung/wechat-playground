{
    "name": "mingyoung/wechat-playground",
    "description": "WeChat Playground",
    "keywords": [
        "wechat",
        "weixin",
        "easywechat"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "张铭阳",
            "email": "mingyoungcheung@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "laravel/framework": "^5.6 || ^6.0",
        "overtrue/wechat": "^4.1"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8 || ^4.0",
        "phpunit/phpunit": "^8.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "WeChatPlayground\\WeChatPlaygroundServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "WeChatPlayground\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WeChatPlayground\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
